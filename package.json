{
  "name": "booking-store",
  "private": true,
  "sideEffects": false,
  "version": "1.0.6",
  "type": "commonjs",
  "scripts": {
    "tunnel": "ngrok http --domain=deciding-seasnail-seriously.ngrok-free.app 3000",
    "build": "shopify hydrogen build",
    "dev": "shopify hydrogen dev --codegen",
    "preview": "npm run build && shopify hydrogen preview",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx .",
    "typecheck": "tsc --noEmit",
    "codegen": "shopify hydrogen codegen",
    "fetch-openapi": "curl -o ./openapi.yaml https://raw.githubusercontent.com/jamalsoueidan/booking-api/main/docs/openapi.yaml",
    "build-openapi": "npx rimraf app/lib/api/model && npx orval --config ./orval.config.js",
    "fb-openapi": "npm run fetch-openapi & npm run build-openapi",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:open": "start-server-and-test dev http://localhost:3000 cypress:open",
    "cy:run": "start-server-and-test preview http://localhost:3000 cypress:run"
  },
  "prettier": "@shopify/prettier-config",
  "dependencies": {
